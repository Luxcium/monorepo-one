  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "db:start": "bash ./scripts/start_docker.sh",
    "predev": "pnpm db:start && pnpm prisma:set",
    "dev": " (sleep 4; xdg-open http://localhost:3000) & next dev",
    "predev:clean": "rimraf .next",
    "dev:clean": "pnpm predev:clean && pnpm dev",
    "eslint": "eslint . --ext .ts,.tsx --cache --cache-location .eslintcache",
    "postinstall": "pnpx prisma generate && pnpm prisma:push",
    "lint": "next lint",
    "prisma:generate": "pnpx prisma generate",
    "prisma:push": "pnpx prisma db push",
    "prisma:set": "pnpm prisma:validate && pnpm prisma:generate && pnpm prisma:push",
    "prisma:validate": "pnpx prisma validate",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "studio": "pnpx prisma studio",
    "test": "jest --passWithNoTests --coverage --watch"
  },
